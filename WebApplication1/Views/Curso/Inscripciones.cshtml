@{
    ViewData["Title"] = "Inscripciones";
}

<h1>Menu de Cursos</h1>

<table id="tabCursos" class="table table-dark table-hover">
    <th>Curso</th>
    <th>Año Calendario</th>
    <th>Condicion</th>
    <th>Nota</th>

    @foreach(var i in ViewBag.inscripciones)
            {
<tr>
    <td>@i.Item2.Descripcion</td>
    <td>@i.Item2.AnioCalendario</td>
    <td>@i.Item1.Condicion</td>
    <td>@i.Item1.Nota</td>
</tr>
            }


</table>
